{
  "name": "electron-pharmacy",
  "productName": "Pharmacy Desktop Application",
  "version": "1.0.0",
  "description": "Desktop application for tracking information about clients in pharmacy created by Grigoris Mavridakis",
  "homepage": "https://github.com/gmavridakis/Electron-PharmacyDestopApplication",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --out=release-builds --asar --prune=true --icon=assets/icons/logo.ico",
    "package-win": "electron-packager . --overwrite --platform=win32 --out=release-builds --asar --prune=true --icon=assets/icons/logo.ico"
  },
  "keywords": [
    "desktopapp",
    "electron"
  ],
  "author": "Grigoris Mavridakis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gmavridakis/Electron-PharmacyDestopApplication.git"
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "jquery": "^3.4.1"
  },
  "dependencies": {
    "asar": "^3.0.2",
    "bootstrap": "^4.1.3",
    "bootstrap-table": "^1.16.0",
    "datatables.net": "^1.10.20",
    "datatables.net-dt": "^1.10.20",
    "electron-packager": "^14.2.1",
    "jQuery": "^1.7.4",
    "mdbootstrap": "^4.14.1",
    "nedb": "^1.8.0",
    "popper.js": "^1.14.4",
    "prune": "0.0.2"
  }
}
